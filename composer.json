{
    "name": "laminas/laminas-router",
    "description": " ",
    "license": "BSD-3-Clause",
    "keywords": [
        "laminas",
        "mvc",
        "routing"
    ],
    "homepage": "https://laminas.dev",
    "support": {
        "docs": "https://docs.laminas.dev/laminas-router/",
        "issues": "https://github.com/laminas/laminas-router/issues",
        "source": "https://github.com/laminas/laminas-router",
        "rss": "https://github.com/laminas/laminas-router/releases.atom",
        "chat": "https://laminas.dev/chat",
        "forum": "https://discourse.laminas.dev"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev",
            "dev-develop": "3.1-dev"
        },
        "laminas": {
            "component": "Laminas\\Router",
            "config-provider": "Laminas\\Router\\ConfigProvider"
        }
    },
    "require": {
        "php": "^5.5 || ^7.0",
        "container-interop/container-interop": "^1.1",
        "laminas/laminas-http": "^2.5",
        "laminas/laminas-servicemanager": "^2.7.5 || ^3.0.3",
        "laminas/laminas-stdlib": "^2.7.5 || ^3.0",
        "laminas/laminas-zendframework-bridge": "^1.0"
    },
    "require-dev": {
        "laminas/laminas-i18n": "^2.6",
        "phpunit/phpunit": "^4.5",
        "sebastian/version": "^1.0.4",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "conflict": {
        "laminas/laminas-mvc": "<3.0.0"
    },
    "suggest": {
        "laminas/laminas-i18n": "^2.6, if defining translatable HTTP path segments"
    },
    "autoload": {
        "psr-4": {
            "Laminas\\Router\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaminasTest\\Router\\": "test/"
        }
    },
    "scripts": {
        "check": [
            "@cs-check",
            "@test"
        ],
        "upload-coverage": "coveralls",
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-clover clover.xml"
    },
    "replace": {
        "zendframework/zend-router": "self.version"
    }
}
