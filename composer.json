{
    "name": "laminas/laminas-router",
    "description": " ",
    "license": "BSD-3-Clause",
    "keywords": [
        "laminas",
        "mvc"
    ],
    "homepage": "https://laminas.dev",
    "support": {
        "docs": "https://docs.laminas.dev/laminas-router/",
        "issues": "https://github.com/laminas/laminas-router/issues",
        "source": "https://github.com/laminas/laminas-router",
        "rss": "https://github.com/laminas/laminas-router/releases.atom",
        "chat": "https://laminas.dev/chat",
        "forum": "https://discourse.laminas.dev"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.7-dev",
            "dev-develop": "2.8-dev"
        }
    },
    "require": {
        "php": "^5.5 || ^7.0",
        "container-interop/container-interop": "^1.1",
        "laminas/laminas-http": "^2.5",
        "laminas/laminas-servicemanager": "^2.7.5 || ^3.0.3",
        "laminas/laminas-stdlib": "^2.7.5 || ^3.0",
        "laminas/laminas-zendframework-bridge": "^1.0"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "1.7.*",
        "laminas/laminas-console": "^2.6",
        "laminas/laminas-filter": "^2.6.1",
        "laminas/laminas-i18n": "^2.6",
        "laminas/laminas-uri": "^2.5",
        "laminas/laminas-validator": "^2.6",
        "phpunit/phpunit": "^4.5",
        "sebastian/version": "^1.0.4"
    },
    "suggest": {
        "laminas/laminas-console": "Laminas\\Console component",
        "laminas/laminas-filter": "Laminas\\Filter component",
        "laminas/laminas-i18n": "Laminas\\I18n component for translatable segments",
        "laminas/laminas-validator": "Laminas\\Validator component"
    },
    "autoload": {
        "psr-4": {
            "Laminas\\Mvc\\Router\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaminasTest\\Mvc\\Router\\": "test/"
        }
    },
    "scripts": {
        "check": [
            "@cs-check",
            "@test"
        ],
        "upload-coverage": "coveralls",
        "cs-check": "php-cs-fixer --version && php-cs-fixer fix -v --diff --dry-run",
        "cs-fix": "php-cs-fixer fix -v",
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-clover clover.xml"
    },
    "replace": {
        "zendframework/zend-router": "self.version"
    }
}
